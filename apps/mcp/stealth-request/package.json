{
  "name": "stealth-request",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "bash scripts/setup.sh",
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "tsc": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "server": "bun run server.ts",
    "server:dev": "bun --watch run server.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "turndown": "^7.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/turndown": "^5.0.6"
  },
  "peerDependencies": {
    "puppeteer": ">=21.0.0"
  }
}
