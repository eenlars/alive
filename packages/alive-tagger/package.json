{
  "name": "@alive-game/alive-tagger",
  "version": "0.1.4",
  "description": "Element selection and source tagging for Alive sandbox",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsgo --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome lint ./src ./test",
    "format": "biome format --write ./src ./test",
    "ci": "biome ci ./src ./test"
  },
  "keywords": [
    "vite",
    "plugin",
    "react",
    "element-selection",
    "source-tagging",
    "alive"
  ],
  "author": "WebAlive",
  "license": "MIT",
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.10",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vitest": "4.0.17"
  }
}
