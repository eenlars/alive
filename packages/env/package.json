{
  "name": "@webalive/env",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "default": "./src/server.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./src/schema.ts",
      "default": "./src/schema.ts"
    }
  },
  "scripts": {
    "build": "tsc --noEmit",
    "dev": "tsc --watch --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "ci": "biome ci ."
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.11.1",
    "dotenv": "^17.0.0",
    "zod": "~3.25.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "typescript": "5.9.3"
  }
}
