    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@napi-rs/image": "^1.9.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "@vitest/coverage-v8": "^2.1.8",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
